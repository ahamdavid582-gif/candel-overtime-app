{
  "name": "candel-overtime-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:all": "concurrently \"npm:dev\" \"wait-on http://127.0.0.1:5173 && npm:electron:dev\"",
    "check-console": "node ./scripts/check-console.cjs",
    "build": "vite build",
    "electron:build": "npm run build && electron-builder",
    "electron:dist": "npm run build && electron-builder --win --x64",
    "electron:dev": "electron .",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "ag-grid-community": "^34.3.1",
    "ag-grid-react": "^34.3.1",
    "firebase": "^12.6.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "jspreadsheet-ce": "^5.0.4",
    "lucide-react": "^0.554.0",
    "react": "^19.2.0",
    "react-data-grid": "^7.0.0-beta.58",
    "react-dom": "^19.2.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "electron": "^26.2.0",
    "electron-builder": "^24.7.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "playwright": "^1.57.0",
    "sharp": "^0.34.5",
    "png-to-ico": "^1.1.0",
    "vite": "^7.2.4",
    "wait-on": "^9.0.3"
  },
  "scripts": {
    "prepare-icons": "node ./scripts/download-icon.js && node ./scripts/generate-icons.js && node ./scripts/generate-electron-icon.js"
  },
  "main": "electron/main.cjs",
  "build": {
    "appId": "com.candel.overtime",
    "productName": "CandelOvertime",
    "icon": "build/icon.ico",
    "files": [
      "dist/**",
      "electron/**",
      "node_modules/**"
    ],
    "directories": {
      "output": "dist-electron"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    }
  }
}
